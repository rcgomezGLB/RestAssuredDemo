{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "data",
    "support"
  ],
  "properties": {
    "data": {
      "type": "object",
      "default": {},
      "title": "The data Schema",
      "required": [
        "id",
        "email",
        "first_name",
        "last_name",
        "avatar"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "default": 0,
          "title": "The id Schema"
        },
        "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema"
        },
        "first_name": {
          "type": "string",
          "default": "",
          "title": "The first_name Schema"
        },
        "last_name": {
          "type": "string",
          "default": "",
          "title": "The last_name Schema"
        },
        "avatar": {
          "type": "string",
          "default": "",
          "title": "The avatar Schema"
        }
      }
    },
    "support": {
      "type": "object",
      "default": {},
      "title": "The support Schema",
      "required": [
        "url",
        "text"
      ],
      "properties": {
        "url": {
          "type": "string",
          "default": "",
          "title": "The url Schema"
        },
        "text": {
          "type": "string",
          "default": "",
          "title": "The text Schema"
        }
      }
    }
  }
}